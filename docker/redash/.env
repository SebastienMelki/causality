# Redash Environment Configuration
PYTHONUNBUFFERED=0
REDASH_LOG_LEVEL=INFO
REDASH_REDIS_URL=redis://redis:6379/0
REDASH_DATABASE_URL=postgresql://hive:hive@postgres:5432/redash

# Security Settings
REDASH_COOKIE_SECRET=causality-dev-cookie-secret
REDASH_SECRET_KEY=causality-dev-secret

# Feature Flags
REDASH_FEATURE_DISABLE_REFRESH_QUERIES=false
REDASH_FEATURE_SHOW_QUERY_RESULTS_COUNT=true

# Authentication Settings
REDASH_PASSWORD_LOGIN_ENABLED=true

# Default Admin Account
REDASH_ADMIN_EMAIL=admin@causality.local
REDASH_ADMIN_PASSWORD=admin123
